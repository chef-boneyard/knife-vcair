source "https://rubygems.org"

# Specify your gem's dependencies in knife-<yourcloud>.gemspec
gemspec

group :development do
  gem 'mixlib-shellout'
  gem 'knife-windows', :git => 'https://github.com/opscode/knife-windows.git'

  gem 'rspec-instafail'
  gem 'debugger'
  gem 'pry-debugger'
  gem 'pry-stack_explorer'
  gem 'dotenv'
end
